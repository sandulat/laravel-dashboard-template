{
  "name": "sandulat/laravel-dashboard-template",
  "description": "Basic Laravel Dashboard Template",
  "keywords": [
    "sandulat",
    "laravel-dashboard-template"
  ],
  "homepage": "https://github.com/sandulat/laravel-dashboard-template",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Stratulat Alexandru",
      "email": "alexanderstratulat97@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.1",
    "illuminate/support": "5.8.*",
    "laravel/framework": "~5.8.0|~5.9.0"
  },
  "require-dev": {
    "orchestra/testbench": "3.8.*",
    "phpunit/phpunit": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "Sandulat\\LaravelDashboardTemplate\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Sandulat\\LaravelDashboardTemplate\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Sandulat\\LaravelDashboardTemplate\\LaravelDashboardTemplateServiceProvider"
      ],
      "aliases": {
        "LaravelDashboardTemplate": "Sandulat\\LaravelDashboardTemplate\\LaravelDashboardTemplateFacade"
      }
    }
  }
}